#! /bin/sh
reldir=`dirname $0`
export ANT_HOME="$reldir/../ant/"
propertyfile=""
props=""
target=$1
if [ "$2" == "-propertyfile" ] ; then
	propertyfile="-propertyfile $3"
	shift
	shift
	shift
fi
shift
for var in "$@"
do
    props="$props -D$var"
done
#echo $reldir/../ant/bin/ant -nouserlib -f $reldir/build.xml $target $props
/bin/sh $reldir/../ant/bin/ant $propertyfile -nouserlib -f build/build.xml $target -Dcfdistro.build.file=$reldir/../build.xml $props
		