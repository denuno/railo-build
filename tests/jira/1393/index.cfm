<cfsetting showdebugoutput="no">
<!---- Initialize data. ------>
<cfset charData = "">
<!----- Create string of ASCII characters (32-255) and concatenate
them. ---->
<cfloop index = "data" from = "32" to = "255">
   <cfset ch = chr(data)>
   <cfset charData = charData & ch>
</cfloop>


<cfset data64 = toBase64(charData)>
<cf_valueEquals 
	left="#data64#" 
    right="">


<cf_valueEquals 
	left="#toString(toBinary(data64))#" 
    right="#charData#">